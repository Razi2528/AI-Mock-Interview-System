# Copy this file to `backend/.env` and update values for your environment
# IMPORTANT: never commit your real `.env` with secrets to version control

# Secret used to sign JWTs
SECRET_KEY=change-me-to-a-secure-random-value
ALGORITHM=HS256
# Token expiry in minutes (default 7 days = 10080)
ACCESS_TOKEN_EXPIRE_MINUTES=10080

# MongoDB connection
MONGODB_URL=mongodb://localhost:27017/
DB_NAME=ai_mock_interview
